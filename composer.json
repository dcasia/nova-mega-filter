{
    "name": "digital-creative/nova-mega-filter",
    "description": "Allows you to control the columns and filters shown on your nova resources.",
    "keywords": [
        "laravel",
        "nova",
        "filter",
        "columns",
        "big-filter"
    ],
    "authors": [
        {
            "name": "Rafael Milewski"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalCreative\\MegaFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCreative\\MegaFilter\\MegaFilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
